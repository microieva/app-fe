{
  "name": "app-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "update":"ng update",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "coverage": "nyc report --reporter=html && open coverage/index.html",
    "build:coverage": "ng build --configuration=production --source-map",
    "cypress:run": "npm run build:coverage && cypress run"
  },
  "nyc": {
    "report": [
      "html",
      "text-summary"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "cypress/**/*",
      "src/main.ts",
      "src/polyfills.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@apollo/client": "^3.10.6",
    "@briebug/jest-schematic": "^6.0.0",
    "@fullcalendar/angular": "^6.1.14",
    "@fullcalendar/core": "^6.1.14",
    "@fullcalendar/daygrid": "^6.1.14",
    "@fullcalendar/interaction": "^6.1.14",
    "@fullcalendar/list": "^6.1.14",
    "@fullcalendar/timegrid": "^6.1.14",
    "@microsoft/signalr": "^8.0.7",
    "apollo-angular": "^7.0.2",
    "graphql": "^16.9.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.4",
    "ngx-editor": "^17.5.4",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.0",
    "tslib": "^2.3.0",
    "typescript": "^5.4.5",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.0",
    "@cypress/code-coverage": "^3.13.5",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.4.2",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^13.15.1",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nyc": "^17.1.0",
    "ts-node": "^10.9.2"
  }
}
